{
	"type": "object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required": true,
	"description": "Schema for requests from Saxs Leash to Saxs Server",
	"additionalProperties": false,
	"properties": 
	{
		"result": 
		{
			"type": "string",
			"required": true
		},

		"data": 
		{
			"required": true,
			"type": "object",
			"additionalProperties": false,
			"properties": 
			{
				"cal": 
				{
					"type": "object"
				},

				"Error": 
				{
					
				},

				"directory": 
				{
					"description": "Directory this queue is going to use. New files in other directories are going to be ignored.",
					"type": "array",
					"default": 
					[
						".",
						"",
						""
					],

					"items": 
					[
						{
							"type": "string"
						}
					]
				},

				"attachments": 
				{
					
				},

				"threads": 
				{
					"type": "integer"
				},

				"dircontent": 
				{
					
				},

				"history": 
				{
					
				},

				"stat": 
				{
					"additionalProperties": false,
					"properties": 
					{
						"queue length": 
						{
							"type": "integer"
						},

						"images processed": 
						{
							"type": "integer"
						},

						"time": 
						{
							"type": "number"
						},

						"start time": 
						{
							"type": "number"
						}
					}
				},

				"filename": 
				{
					"type": "string"
				},

				"graphs": 
				{
					"type": "array",
					"items": 
					{
						"type": "object",
						"properties": 
						{
							"kind": 
							{
								"type": "string",
								"enum": 
								[
									"Radial",
									"Slice"
								]
							},

							"conf": 
							{
								"type": "object"
							},

							"columnLabels": 
							{
								"type": "array",
								"items": 
								{
									"type": "string"
								}
							},

							"array": 
							{
								"type": "array",
								"items": 
								[
									{
										"type": "array",
										"items": 
										{
											"type": "number"
										}
									}
								]
							}
						}
					}
				}
			}
		}
	}
}