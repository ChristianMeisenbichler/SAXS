{
	"type": "object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required": true,
	"description": "Schema for requests from Saxs Leash to Saxs Server",
	"additionalProperties": false,
	"properties": 
	{
		"result": 
		{
			"type": "string",
			"required":true
		},

		"data": 
		{	"required":true,
			"type": "object",
			"properties": 
			{
				"queue length": 
				{
					"type": "integer"
				},

				"images processed": 
				{
					"type": "integer"
				},

				"queue id": 
				{
					"type": "string"
				},

				"file name": 
				{
					"type": "string"
				},

				"header": 
				{
					"type": "array",
					"items": 
					[
						{
							"type": "string"
						}
					]
				},

				"array": 
				{
					"type": "array",
					"items": 
					[
						{
							"type": "array",
							"items": 
							[
								{
									"type": "number"
								}
							]
						}
					]
				}
			}
		}
	}
}