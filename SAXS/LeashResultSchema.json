{
	"type": "object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required": true,
	"description": "Schema for requests from Saxs Leash to Saxs Server",
	"additionalProperties": false,
	"properties": 
	{
		"result": 
		{
			"type": "string",
			"required": true
		},

		"data": 
		{
			"required": true,
			"type": "object",
			"additionalProperties": false,
			"properties": 
			{
				"stat": 
				{
					"additionalProperties": false,
					"properties": 
					{
						"queue length": 
						{
							"type": "integer"
						},

						"images processed": 
						{
							"type": "integer"
						},

						"time interval": 
						{
							"type": "number"
						},

						"frames per sec": 
						{
							"type": "number"
						},

						"pics": 
						{
							"type": "integer"
						}
					}
				},

				"filename": 
				{
					"type": "string"
				},

				"array": 
				{
					"type": "array",
					"items": 
					[
						{
							"type": "array",
							"items": 
							[
								{
									"type": "number"
								}
							]
						}
					]
				}
			}
		}
	}
}